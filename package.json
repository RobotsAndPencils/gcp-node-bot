{
  "name": "ea-support-bot",
  "version": "1.0.0",
  "description": "EA Support bot",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "npm run lint && npm run coverage",
    "lint": "standard",
    "unit": "ava --verbose --serial",
    "coverage": "nyc --check-coverage --statements 80 ava --verbose --serial",
    "lcov": "nyc --reporter lcov ava --serial",
    "watch": "ava --verbose --serial --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobotsAndPencils/ea-support-bot.git"
  },
  "keywords": [
    "ea",
    "slackbot"
  ],
  "author": "Curtis Allen <curtis.allen@robotsandpencils.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RobotsAndPencils/ea-support-bot/issues"
  },
  "homepage": "https://github.com/RobotsAndPencils/ea-support-bot#readme",
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "beepboop-slapp-presence-polyfill": "^1.1.1",
    "express": "^4.14.0",
    "slapp": "^1.2.0",
    "slapp-context-beepboop": "^1.0.0",
    "slapp-convo-beepboop": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "dox": "^0.8.1",
    "nodemon": "^1.10.2",
    "nyc": "^7.0.0",
    "sinon": "^1.17.4",
    "standard": "^7.1.2"
  }
}
